<header>
    <h1>{{ shop.name }}</h1>

    <ul>
        {% for link in linklists.main-menu.links -%}
            <li> {{ link.title | link_to: link.url }}</li>
        {%- endfor %}
    </ul>
    <form action="/search">
        <label>
            <input type="search" name="q"/>
        </label>
        <button type="submit">Search</button>
    </form>
</header>